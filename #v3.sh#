#!/bin/bash -x
isPartTime=1
isFullTime=2
empWages=20
randomCheck=$((RANDOM%3))
case  $randomCheck in
    pattern1)
	if [ $isF -eq $randomCheck ]
	then
	    emphours=8
	    salary=$(($empWages*$empHours))
	    echo $salary
	fi;;
    pattern2)
	if [ $isP -eq $randomCheck ]
	then
	    empHours=4
	    salary=$(($empWages*$empHours))
	    echo $salary
	fi;;
esac


